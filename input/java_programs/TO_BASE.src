package java_programs; import java.util.*; /** * @author derricklin */public class TO_BASE { public static String to_base(int num, int b) { String result = ""; String alphabet = "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ"; int i; while (num > 0) { i = num % b; // floor division? num = num / b; result = result + String.valueOf(alphabet.charAt(i)); } return result; }} /* * To change this template, choose Tools | Templates * and open the template in the editor. */ 
